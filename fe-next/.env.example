# Frontend Next.js Configuration
# Copy this file to .env.local and adjust values as needed

# Orders API URL
# Use the URL where your orders-api-python service is running
NEXT_PUBLIC_API_URL=http://localhost:8080

# Payment Service URL
# Use the URL where your payment-service-nestjs is running
NEXT_PUBLIC_PAYMENT_API_URL=http://localhost:3001

# Aggregator Service URL
# Use the URL where your aggregator-service is running
NEXT_PUBLIC_AGGREGATOR_URL=http://localhost:8081
